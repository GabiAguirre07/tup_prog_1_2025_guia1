Algoritmo combustible
	Definir km,combust,combustkm como Real;
	Escribir " Ingrese la cantidad de combustile que posee en litros: ";
	Leer combust;
	Escribir " Ingrese la cantidad de km recorridos: ";
	Leer km;
	combustkm <- km/combust;
	Escribir " El combustle consumido por kilometro recorrido es de ",combustkm;
FinAlgoritmo
